Resources:
  AWSEBAutoScalingGroup:
    Metadata:
      AWS::CloudFormation::Authentication:
        S3Auth:
          type: "s3"
          buckets: ["notejamcreds"]
          roleName: "aws-elasticbeanstalk-ec2-role"

files:
  "/tmp/db.json" :
    mode: "000644"
    owner: root
    group: root
    authentication: "S3Auth"
    source: https://notejamcreds.s3-us-west-1.amazonaws.com/db.json
